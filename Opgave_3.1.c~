#include <stdio.h>
#include <math.h>

int main(void)
{
  double r,x,y,hypotenusen;

  printf("Bestem radius på din cirkel: \n");
  scanf("%lf",&r);

  printf("Bestem dine koordinater x og y: \n");
  scanf("%lf %lf",&x,&y);

  hypotenusen = square((x*x)+(y*y))

  if(hypotenusen <= r)
   printf("Punktet befinder sig inden i eller på cirklen");
  else
   print("Punktet befinder sig udenfor cirklen");

  return 0;
}
